# EASY-INSTALL-ENTRY-SCRIPT: 'technicolor==2017.1.16.1544','console_scripts','technicolor'
__requires__ = 'technicolor==2017.1.16.1544'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('technicolor==2017.1.16.1544', 'console_scripts', 'technicolor')()
    )
